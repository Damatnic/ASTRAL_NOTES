{
  "name": "astral-notes-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:suite": "node test-runner.cjs run",
    "test:category": "node test-runner.cjs category",
    "test:master": "node test-runner.cjs run master",
    "test:ai-services": "node test-runner.cjs run ai-services",
    "test:routing": "node test-runner.cjs run routing",
    "test:quick-notes": "node test-runner.cjs run quick-notes",
    "test:project-management": "node test-runner.cjs run project-management",
    "test:enhanced-editor": "node test-runner.cjs run enhanced-editor",
    "test:performance": "node test-runner.cjs run performance",
    "test:accessibility": "node test-runner.cjs run accessibility",
    "test:unit": "node test-runner.cjs category unit",
    "test:integration": "node test-runner.cjs category integration",
    "test:all": "node test-runner.cjs run all",
    "test:report": "node test-runner.cjs report",
    "test:validate": "node test-runner.cjs validate",
    "test:list": "node test-runner.cjs list",
    "test:parallel": "node test-runner.cjs category all --parallel --coverage",
    "test:ci": "node test-runner.cjs run all --ci --coverage --reporter=json,junit",
    "stellar:check": "npm run typecheck && npm run lint && npm run test:unit",
    "stellar:build": "npm run stellar:check && npm run build",
    "stellar:dev": "npm run typecheck && npm run dev",
    "stellar:test": "npm run test:master --coverage --reporter=html",
    "cosmic:clean": "rm -rf dist node_modules/.vite test-results coverage",
    "cosmic:fresh": "npm run cosmic:clean && npm install && npm run stellar:build",
    "cosmic:test": "npm run test:all --parallel --coverage"
  },
