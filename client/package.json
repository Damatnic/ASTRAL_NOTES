{
  "name": "astral-notes-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "stellar:check": "npm run typecheck && npm run lint && npm run test:run",
    "stellar:build": "npm run stellar:check && npm run build",
    "stellar:dev": "npm run typecheck && npm run dev",
    "cosmic:clean": "rm -rf dist node_modules/.vite",
    "cosmic:fresh": "npm run cosmic:clean && npm install && npm run stellar:build"
  },
